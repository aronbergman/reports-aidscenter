    "@sentry/node": "^5.15.5",
    "antd": "^4.19.5",
    "antd-mask-input": "^2.0.7",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.1.3",
    "concurrently": "^5.2.0",
    "consola": "^2.12.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.28.0",
    "dotenv": "^8.2.0",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "0.0.1-security",
